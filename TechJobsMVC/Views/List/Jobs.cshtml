<div class="container body-content">

    <h1>@ViewBag.title</h1>

   
    @foreach (Job job in ViewBag.jobs)
    {
            <table class="job-listing">
                <tbody>
                    <tr>
                        <td>
                            ID:  @job.Id
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Name:  @job.Name
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Employer:  @job.Employer
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Location:  @job.Location
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Position:  @job.PositionType
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Compentency:  @job.CoreCompetency
                        </td>
                    </tr>
                </tbody>
            </table>
        
            
    }




</div>